generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["mongoDb"]
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model goal_types {
  id          String @id @default(dbgenerated()) @map("_id") @db.ObjectId
  description String
  name        String

  @@map("goal-types")
}

model goals {
  id   String @id @default(dbgenerated()) @map("_id") @db.ObjectId
  name String
  type String @db.ObjectId
}
